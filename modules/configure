#endif
#include <sys/resource.h>
  main(){struct rlimit r;r.rlim_cur=-1;exit(r.rlim_cur<0);}
_ACEOF
if ac_fn_c_try_run "$LINENO"; then :
  zsh_cv_type_rlim_t_is_unsigned=yes
else
